{
    "compilerOptions": {
        "module": "es2022",
        "lib": ["es2022", "DOM"],
        "target": "es2022",
        "outDir": "./dist",
        "moduleResolution": "node",
        "skipLibCheck": true,
        "jsx": "react",
        "noImplicitAny": true,
        "sourceMap": true,
        "strictNullChecks": true
    },
    "include": ["**/*.ts", "**/*.tsx"],
    "exclude": ["node_modules"]
}
